{
  "name": "webpack-ready",
  "version": "1.0.0",
  "description": "a ready to go project skeleton based on webpack",
  "main": "index.js",
  "scripts": {
    "test": "mocha test --compilers js:babel-core/register",
    "cover": "nyc npm t",
    "start": "webpack-dev-server --hot --config config/webpack.dev.config.js --content-base public --watch",
    "lint": "eslint src/**/*.js",
    "build": "webpack --config config/webpack.dev.config.js",
    "build:prod": "webpack -p --config config/webpack.prod.config.js"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.5.3",
    "nyc": "^7.0.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrei-cacio/webpack-ready.git"
  },
  "author": "Andrei Cacio <andrei.cacio@yahoo.com> (http://andreicacio.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrei-cacio/webpack-ready/issues"
  },
  "homepage": "https://github.com/andrei-cacio/webpack-ready#readme"
}
